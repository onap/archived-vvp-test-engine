########################################################################################################################
# Set various ENV parameters for the ICE CI Application.
########################################################################################################################
DJANGO_SETTINGS_MODULE=settings
PYTHONPATH=/app
DISPLAY=:99
LOCAL_TIME_ZONE=UTC-03:00
ICE_CI_ENVIRONMENT_NAME=Dev
NUMBER_OF_TEST_RESULTS=30

############################
# ICE-CI Database Settings
############################
CI_DB_NAME={PLACE_HOLDER}
CI_DB_USER={PLACE_HOLDER}
CI_DB_PASSWORD={PLACE_HOLDER}
CI_DB_HOST={PLACE_HOLDER}
CI_DB_PORT=5432

############################
# ICE-EM Database Settings
############################
ICE_DB_NAME={PLACE_HOLDER}
ICE_DB_USER={PLACE_HOLDER}
ICE_DB_PASSWORD={PLACE_HOLDER}
ICE_DB_HOST={PLACE_HOLDER}
ICE_DB_PORT=5432

############################
# ICE-CI Contact Mail Settings
############################
ICE_EMAIL_HOST={PLACE_HOLDER}
ICE_CONTACT_EMAILS={PLACE_HOLDER}
ICE_CONTACT_FROM_ADDRESS={PLACE_HOLDER}

############################
# ICE-CI Logger (Levels: [DEBUG, INFO, WARNING, ERROR, CRITICAL])
############################
ICE_ICE_LOGGER_LEVEL=DEBUG
ICE_DJANGO_LOGGER_LEVEL=DEBUG
ICE_ICE_LOGGER_PATH=/app/logs/

############################
#HTTP
############################
ICE_PORTAL_URL={PLACE_HOLDER}
